<?xml version="1.0" encoding="UTF-8"?>
<kcfg xmlns="http://www.kde.org/standards/kcfg/1.0"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.kde.org/standards/kcfg/1.0
      http://www.kde.org/standards/kcfg/1.0/kcfg.xsd" >
  <kcfgfile name=""/>

  <group name="General">
    <entry name="expanding" type="Bool">
        <label>If true, the spacer tries to take all the available space in the panel.</label>
        <default>true</default>
    </entry>
    <entry name="length" type="Int">
        <label>length in pixels of the spacer. Configuration effective only if expanding is set to false. A negative value means an invalid value that should be completely ignored.</label>
        <default>-1</default>
    </entry>

    <entry name="singleClickAction" type="String">
      <default>Overview,kwin</default>
    </entry>
    <entry name="singleClickCommand" type="String">
      <default>date="$(date -u)"; notify-send "Single Click" "$date"</default>
    </entry>
    <entry name="singleClickAppUrl" type="String">
      <default></default>
    </entry>

    <entry name="doubleClickAction" type="String">
      <default>Window Maximize,kwin</default>
    </entry>
    <entry name="doubleClickCommand" type="String">
      <default>date="$(date -u)"; notify-send "Double Click" "$date"</default>
    </entry>
    <entry name="doubleClickAppUrl" type="String">
      <default></default>
    </entry>


    <entry name="middleClickAction" type="String">
      <default>Show Desktop,kwin</default>
    </entry>
    <entry name="middleClickCommand" type="String">
      <default>date="$(date -u)"; notify-send "Middle Click" "$date"</default>
    </entry>
    <entry name="middleClickAppUrl" type="String">
      <default></default>
    </entry>

    <entry name="mouseWheelUpAction" type="String">
      <default>Switch to Previous Desktop,kwin</default>
    </entry>
    <entry name="mouseWheelUpCommand" type="String">
      <default>date="$(date -u)"; notify-send "Wheel Up" "$date"</default>
    </entry>
    <entry name="smouseWheelUpAppUrl" type="String">
      <default></default>
    </entry>

    <entry name="mouseWheelDownAction" type="String">
      <default>Switch to Next Desktop,kwin</default>
    </entry>
    <entry name="mouseWheelDownCommand" type="String">
      <default>date="$(date -u)"; notify-send "Wheel Down" "$date"</default>
    </entry>
    <entry name="mouseWheelDownAppUrl" type="String">
      <default></default>
    </entry>
    

    <entry name="mouseDragUpAction" type="String">
      <default>Window Move,kwin</default>
    </entry>
    <entry name="mouseDragUpCommand" type="String">
      <default>date="$(date -u)"; notify-send "Drag Up" "$date"</default>
    </entry>
    <entry name="mouseDragUpAppUrl" type="String">
      <default></default>
    </entry>

    <entry name="mouseDragDownAction" type="String">
      <default>Window Move,kwin</default>
    </entry>
    <entry name="mouseDragDownCommand" type="String">
      <default>date="$(date -u)"; notify-send "Drag Down" "$date"</default>
    </entry>
    <entry name="mouseDragDownAppUrl" type="String">
      <default></default>
    </entry>

    <entry name="mouseDragLeftAction" type="String">
      <default>Window One Desktop to the Left,kwin</default>
    </entry>
    <entry name="mouseDragLeftCommand" type="String">
      <default>date="$(date -u)"; notify-send "Drag Left" "$date"</default>
    </entry>
    <entry name="mouseDragLeftAppUrl" type="String">
      <default></default>
    </entry>

    <entry name="mouseDragRightAction" type="String">
      <default>Window One Desktop to the Right,kwin</default>
    </entry>
    <entry name="mouseDragRightCommand" type="String">
      <default>date="$(date -u)"; notify-send "Drag Right" "$date"</default>
    </entry>
    <entry name="mouseDragRightAppUrl" type="String">
      <default></default>
    </entry>

    <entry name="pressHoldAction" type="String">
      <default>ExposeAll,kwin</default>
    </entry>
    <entry name="pressHoldActionCommand" type="String">
      <default>date="$(date -u)"; notify-send "Long Press" "$date"</default>
    </entry>
    <entry name="pressHoldAppUrl" type="String">
      <default></default>
    </entry>

    <entry name="enableDebug" type="Bool">
      <default>false</default>
    </entry>

    <entry name="showTooltip" type="Bool">
      <default>true</default>
    </entry>

    <entry name="qdbusCommand" type="String">
      <default>qdbus</default>
    </entry>
  </group>

</kcfg>
